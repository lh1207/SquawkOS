{
  "name": "squawkos",
  "version": "1.0.0",
  "description": "Windows Squawk is a game that parodies the Windows operating system by replacing all its elements with bird-related items.",
  "private": "true",
  "scripts": {
    "test": "jest --watch",
    "start": "webpack-dev-server --progress --colors --hot --inline",
    "dev": "npm-run-all --parallel test start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lh1207/SquawkOS.git"
  },
  "author": "lh1207",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lh1207/SquawkOS/issues"
  },
  "homepage": "https://github.com/lh1207/SquawkOS#readme",
  "dependencies": {
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@eslint/js": "^9.1.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^0.0.0",
    "css-loader": "^7.1.1",
    "eslint": "^9.1.1",
    "file-loader": "^6.2.0",
    "globals": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
